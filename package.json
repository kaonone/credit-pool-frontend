{
  "name": "ethereum-dapp-starter-kit",
  "version": "0.0.1",
  "description": "Frontend starter kit for Ethereum DApps",
  "main": "index.js",
  "scripts": {
    "dev": "npm run dev:sandbox",
    "dev:testnet": "npm run codegen:testnet && cross-env MODE=testnet better-npm-run dev",
    "dev:sandbox": "npm run codegen:sandbox && cross-env MODE=sandbox better-npm-run dev",
    "dev:mainnet": "npm run codegen:mainnet && cross-env MODE=mainnet better-npm-run dev",
    "build:testnet": "cross-env MODE=testnet better-npm-run build",
    "build:sandbox": "cross-env MODE=sandbox better-npm-run build",
    "build:mainnet": "cross-env MODE=mainnet better-npm-run build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx && tsc --noEmit --incremental false --pretty",
    "lint:autofix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest && npm run lint",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand --watch",
    "test:coverage": "jest --coverage --watch",
    "codegen:testnet": "rm -rf ./src/generated && graphql-codegen --config codegen-testnet.yml; typechain --target web3-rxjs --outDir src/generated/contracts **/abi/*.json",
    "codegen:sandbox": "rm -rf ./src/generated && graphql-codegen --config codegen-sandbox.yml; typechain --target web3-rxjs --outDir src/generated/contracts **/abi/*.json",
    "codegen:mainnet": "rm -rf ./src/generated && graphql-codegen --config codegen-mainnet.yml; typechain --target web3-rxjs --outDir src/generated/contracts **/abi/*.json"
  },
  "betterScripts": {
    "node": {
      "command": "node ./node_modules/.bin/cross-env",
      "env": {
        "TS_NODE_PROJECT": "webpack/tsconfig.json",
        "TS_NODE_FILES": true,
        "NODE_OPTIONS": "--max_old_space_size=8192"
      }
    },
    "build": {
      "command": "better-npm-run node webpack --config webpack/prod.ts"
    },
    "dev": {
      "command": "better-npm-run node webpack-dev-server --config webpack/dev.ts"
    }
  },
  "author": "Serdtsev Dmitriy",
  "license": "MIT",
